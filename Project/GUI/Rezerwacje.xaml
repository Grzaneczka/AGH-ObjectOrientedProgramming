<Window x:Class="GUI.Rezerwacje"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="Rezerwacje" Height="450" Width="800">
    <Grid>
        <Label Content="Lista rezerwacji" HorizontalAlignment="Left" Height="55" Margin="35,10,0,0" VerticalAlignment="Top" Width="200"/>
        <ListBox x:Name="ListBox_Rezerwacje" HorizontalAlignment="Left" Height="170" Margin="10,65,0,0" VerticalAlignment="Top" Width="390"/>
        <Button x:Name="Button_DodajRezerwacje" Content="Dodaj" HorizontalAlignment="Left" Height="40" Margin="35,335,0,0" VerticalAlignment="Top" Width="90" Click="Button_DodajRezerwacje_Click"/>
        <Button Content="Powrot" HorizontalAlignment="Left" Height="40" Margin="595,335,0,0" VerticalAlignment="Top" Width="100" Click="Button_Click"/>

    </Grid>
</Window>
